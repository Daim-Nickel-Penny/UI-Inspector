{
  "name": "UI inspector",
  "version": "0.0.1",
  "description": "UI designer's assistant tool",
  "manifest_version": 2,
  "permission": ["activetab"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "UI-Inspector",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": ["styles.css"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
